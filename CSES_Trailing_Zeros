package Introductory_Problems;

import java.util.Scanner;

public class CSES_Trailing_Zeros {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		long ans = 0;
		for(int i = 5 ; i <= n ; i*= 5 ) {
			ans += n/i;
		}
		System.out.println(ans);
		sc.close();
	}

}
