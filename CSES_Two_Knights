package Introductory_Problems;
import java.util.*;
public class CSES_Two_Knights {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		 int n = sc.nextInt();
		    for (int i = 1; i <= n; ++i)
		    {
		        long res = (((long)i-1)*(i+4)*((long)i*i - 3*i + 4)) / 2;
		        System.out.println(res);
		    }
		    sc.close();
	}

}
